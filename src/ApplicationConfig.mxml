<?xml version="1.0" encoding="utf-8"?>
<swiz:BeanLoader
	xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:swiz="http://swiz.swizframework.org"
	xmlns:malibu="com.stdva.malibu.*"
	xmlns:vpaint="com.stdva.malibu.vpaint.*"
	xmlns:gui="com.stdva.malibu.vpaint.gui.*">
	
	<mx:Script>
		<![CDATA[
			import com.stdva.malibu.Bottle;
			import com.stdva.malibu.brushes.b1;
		]]>
	</mx:Script>
	
	
	<malibu:PainterWindow  id="painterWindow"/>
	<vpaint:History id="history" />
	<vpaint:GUIListener id="guiListener" />
	<vpaint:DrawingParams id="drawingParams"/>
	
	<gui:Configurator id="guiConfigurator"/>
	
	<vpaint:MySlider id="widthSlider" scale="{painterWindow.widthSliderScale}" todder="{painterWindow.widthSliderTodder}"/>
	<vpaint:MySlider id="alphaSlider" scale="{painterWindow.alphaSliderScale}" todder="{painterWindow.alphaSliderTodder}"/>
	
	<vpaint:ToolSet id="toolset">
		<vpaint:BDDrawTool id="brush1" brushClass="{com.stdva.malibu.brushes.b1}"/>
		<vpaint:BDDrawTool id="brush2" brushClass="{com.stdva.malibu.brushes.b2}"/>
		<vpaint:BDDrawTool id="brush3" brushClass="{com.stdva.malibu.brushes.b3}"/>
		<vpaint:BDDrawTool id="brush4" brushClass="{com.stdva.malibu.brushes.b4}"/>
		<vpaint:BDDrawTool id="brush5" brushClass="{com.stdva.malibu.brushes.b5}"/>
	</vpaint:ToolSet>
	
</swiz:BeanLoader>
